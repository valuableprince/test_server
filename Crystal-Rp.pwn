		        format(YCMDstr, sizeof(YCMDstr), "[Ŕíňč÷čň]: %s ďîëó÷čë Warn çŕ čńďîëüçîâŕíčĺ ÷čňîâ (%d/3)", name, PI[playerid][pWarns]);
		        SendClientMessageToAll(COLOR_LIGHTRED, YCMDstr);

		        PI[playerid][punWarns] = 1;
		        PI[playerid][punWarnstime] = gettime();
		    }

		    PI[playerid][pMember] = 0;
		    PI[playerid][pLeader] = 0;
		    Kick(playerid);
		}
		/******************************** Ŕíňč÷čň íŕ áűńňđóţ ĺçäó íŕ ěîďĺäĺ *******************************/
		if(IsPlayerInAnyVehicle(playerid) && GetVehicleModel(GetPlayerVehicleID(playerid)) == 462 && SpeedVehicle(playerid) > 110) SCM(playerid, COLOR_LIGHTRED, "Âű áűëč ęčęíóňű ďî ďîäîçđĺíčţ â ÷čňĺđńňâĺ. (#2888)"), Kick(playerid);
	}
	return true;
