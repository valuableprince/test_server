CMD:gethere(playerid, params[])
{
	if(PI[playerid][pLogin] == 0) return true;
	if(PI[playerid][pBSA] < 3 || GetPVarInt(playerid,"dostup") == 0) return true;
	new plo;
	if(sscanf(params, "u",plo)) return SCM(playerid, COLOR_WHITE, "Ââĺäčňĺ: /gethere [id]");
	new Float:plocx,Float:plocy,Float:plocz;
	new inter = GetPlayerInterior(playerid);
	new worldd = GetPlayerVirtualWorld(playerid);
	if(IsPlayerConnected(plo))
	{
		if(plo != INVALID_PLAYER_ID)
		{
			if(PI[playerid][pBSA] >= 3)
			{
				if(GetPlayerState(plo) != 1 && GetPlayerState(plo) != 2 && GetPlayerState(plo) != 3) return	SCM(playerid, COLOR_GRAD1, "Čăđîę íĺ âńňóďčë â čăđó!");
