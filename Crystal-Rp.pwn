		        SendClientMessageToAll(COLOR_LIGHTRED, YCMDstr);

		        PI[playerid][pLocked] = 1;
		        PI[playerid][pBanMounth] = gettime() + (14 * 86400); // 14 äíĺé â ńĺęóíäŕő
		        PI[playerid][pMember] = 0;
			    PI[playerid][pLeader] = 0;
			    PI[playerid][pJob] = 0;
			    PI[playerid][pSong] = 0;
		    }
